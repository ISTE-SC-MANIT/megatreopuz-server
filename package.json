{
  "name": "megatreopuz-server",
  "version": "1.0.0",
  "private": true,
  "description": "The interface between UI and gRPC architecture",
  "scripts": {
    "start": "NODE_ENV=production node lib/index.js",
    "format": "prettier --write \"src/*.ts\"",
    "lint": "eslint \"src/*.ts\"",
    "commit": "commit",
    "build": "tsc",
    "dev": "NODE_ENV=development ts-node --inspect=8000 --no-notify --respawn --transpileOnly src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "nodemon  src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ISTE-SC-MANIT/megatreopuz-server.git"
  },
  "keywords": [
    "megatreopuz"
  ],
  "author": "Yash Mahalwal <yashmahalwal@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ISTE-SC-MANIT/megatreopuz-server/issues"
  },
  "homepage": "https://github.com/ISTE-SC-MANIT/megatreopuz-server#readme",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.5.0",
    "apollo-server": "^3.6.1",
    "apollo-server-core": "^3.6.2",
    "apollo-server-express": "^3.6.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^11.0.0",
    "express": "^4.18.2",
    "firebase-admin": "^9.11.0",
    "graphql": "^15.3.0",
    "moment": "^2.29.1",
    "nanoid": "^3.1.30",
    "redis": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "ts-protoc-gen": "^0.15.0",
    "type-graphql": "^1.1.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/google-protobuf": "^3.15.5",
    "@types/node": "^17.0.8",
    "@types/redis": "^4.0.11",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}
